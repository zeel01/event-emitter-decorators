{
    "targets": {
        "node": "current"
    },
    "plugins": [
        ["@babel/plugin-proposal-decorators", { "version": "2023-11" }],
        ["@babel/plugin-transform-class-static-block"],
        ["polyfill-corejs3", { "method": "usage-pure", "version": "^3.31.0", "proposals": true }]
    ],
    "highlightCode": true,
    "generatorOpts": {
        "importAttributesKeyword": "with"
    }
}